<!DOCTYPE html>
<html>
    <head>
        <title>BarHopper</title>
        <meta name="description" content="">
        <meta name="keywords" content="">
        <meta charset="utf-8">
        <meta name="Tim O'Bara" content="Tim O'Bara">
        <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />

        <!-- Favicons -->
  <!--  <link rel="shortcut icon" href="images/favicon.png">
        <link rel="apple-touch-icon" href="images/apple-touch-icon.png">
        <link rel="apple-touch-icon" sizes="72x72" href="images/apple-touch-icon-72x72.png">
        <link rel="apple-touch-icon" sizes="114x114" href="images/apple-touch-icon-114x114.png"> -->

        <!-- CSS -->
       <%= stylesheet_link_tag    'application' %>
       <%= stylesheet_link_tag    'bootstrap.min.css' %>
       <%= stylesheet_link_tag    'style.css' %>
       <%= stylesheet_link_tag    'style-responsive.css' %>
       <%= stylesheet_link_tag    'animate.min.css' %>
       <%= stylesheet_link_tag    'vertical-rhythm.min.css' %>
       <%= stylesheet_link_tag    'owl.carousel.css' %>
       <%= stylesheet_link_tag    'magnific-popup.css' %>


    </head>
    <body class="appear-animate">

        <!-- Page Loader -->
        <div class="page-loader">
            <div class="loader">Loading...</div>
        </div>
        <!-- End Page Loader -->

        <!-- Page Wrap -->
        <div class="page" id="top">

            <!-- Home Section -->
            <section class="home-section bg-dark-alfa-30 parallax-2" data-background="/assets/black.png" id="home">
                <div class="js-height-full">

                    <!-- Hero Content -->
                    <div class="home-content">
                        <div class="home-text">


                    <div class="row">

                        <div class="col-sm-6">
                            <h1 class="hs-line-1 font-alt mb-80 mb-xs-30 mt-50 mt-sm-0">

              </h1>
                            <div class="mt-140 mt-lg-80 mb-140 mb-lg-80">
                                <div class="banner-content">
                                    <h3 class="banner-heading font-alt">Boston, MA</h3>
                                    <div class="local-scroll">
                                        <%= link_to "Enter", bars_path %>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="col-sm-6 banner-image wow fadeInUp" data-wow-duration="1.2s">

                            <%= image_tag("beer.png") %>
                        </div>

                    </div>
              </div>
                </div>
                </div>
            </section>

            <!-- End Home Section -->

             <!-- Navigation panel -->

            <!-- End Navigation panel -->


        </div>
        <!-- End Page Wrap -->


        <!-- JS -->

        <%= javascript_include_tag 'jquery.barfiller.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery-1.11.2.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.easing.1.3.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'bootstrap.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'SmoothScroll.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.scrollTo.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.localScroll.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.viewport.mini.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.countTo.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.appear.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.sticky.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.parallax-1.1.3.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.fitvids.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'owl.carousel.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'isotope.pkgd.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'imagesloaded.pkgd.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.magnific-popup.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'http://maps.google.com/maps/api/js?sensor=false&amp;language=en' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'gmap3.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'wow.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'masonry.pkgd.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.simple-text-rotator.min.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'all.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'contact-form.js' %> <script type="text/javascript"> </script>
        <%= javascript_include_tag 'jquery.ajaxchimp.min.js' %> <script type="text/javascript"> </script>
        <!--[if lt IE 10]><script type="text/javascript" src="js/placeholder.js"></script><![endif]-->

    </body>
</html>



